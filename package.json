    "angular-animate": "1.5.9",
    "angular-aria": "1.5.9",
    "angular-drag-and-drop-lists": "^2.1.0",
    "angular-file-saver": "^1.1.3",
    "angular-material": "^1.1.1",
    "bootstrap": "^3.3.7",
    "json-schema-faker": "^0.4.0",
    "ng-file-upload": "^12.2.13"
  },
